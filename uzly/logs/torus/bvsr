set terminal postscript eps enhanced color font 'Helvetica,10'
set output 'torusBvsR.eps'

set   autoscale                        # scale axes automatically
unset log                              # remove any log-scaling
unset label   
set pointsize 1

set style line 11 lc rgb '#808080' lt 1
set border 3 back ls 11
set tics nomirror

set logscale

set xlabel 'Pocet krizeni'
set ylabel 'Cas [s]'

plot "torus_log_B.txt" using 2:3 title 'Algoritmus B',\
"torus_log_R.txt" using 2:3 title 'Algoritmus R',\
"torus_log_A.txt" using 2:3 title 'Algoritmus A'

